[install_branch]
- packages = nano

apt("update and install")  
-> update
-> present:
	- names = "{{packages}}"

if(packages == nano)
{
	apt("try to delete")
	-> absent:
		- names = "{{packages}}"

	apt("try to delete again")
	-> absent:
		- names = "{{packages}}"

	apt("install via apt")
	-> present >> test_output:
		- names = "{{packages}}"

	print("test output")
	- output = "{{test_output}}"
}
else
{
    apt(apt1_in_else)
    -> absent:
            - names = "{{packages}}"

    apt(apt2_in_else)
    -> present:
            - names = "{{packages}}"
}
